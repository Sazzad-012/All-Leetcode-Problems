class Solution {
public:
    vector<int> majorityElement(vector<int> & n) {
        map<int, int> mp;
        vector<int> ans;

        for(int x : n) {
            mp[x]++;
        }

        for(auto it : mp) {
            if(it.second > n.size() / 3) {
                ans.push_back(it.first);
            }
        }

        return ans;
    }
};
